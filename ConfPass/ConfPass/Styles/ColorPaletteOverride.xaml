<ResourceDictionary xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
  <ResourceDictionary.ThemeDictionaries>
    <ResourceDictionary x:Key="Light">
      <!-- Neumorphic Base Colors -->
      <Color x:Key="NeumorphicBgColor">#E0E5EC</Color>
      <Color x:Key="NeumorphicShadowDarkColor">#A3B1C6</Color>
      <Color x:Key="NeumorphicShadowLightColor">#FFFFFF</Color>
      <Color x:Key="NeumorphicTextPrimaryColor">#2D3748</Color>
      <Color x:Key="NeumorphicTextSecondaryColor">#5A6578</Color>
      <Color x:Key="NeumorphicTextMutedColor">#8A95A5</Color>
      <Color x:Key="NeumorphicAccentColor">#4A5568</Color>

      <!-- Material Color Overrides (mapped to neumorphic palette) -->
      <Color x:Key="PrimaryColor">#4A5568</Color>
      <Color x:Key="OnPrimaryColor">#FFFFFF</Color>
      <Color x:Key="PrimaryContainerColor">#E0E5EC</Color>
      <Color x:Key="OnPrimaryContainerColor">#2D3748</Color>
      <Color x:Key="SecondaryColor">#5A6578</Color>
      <Color x:Key="OnSecondaryColor">#FFFFFF</Color>
      <Color x:Key="SecondaryContainerColor">#E0E5EC</Color>
      <Color x:Key="OnSecondaryContainerColor">#2D3748</Color>
      <Color x:Key="TertiaryColor">#8A95A5</Color>
      <Color x:Key="OnTertiaryColor">#FFFFFF</Color>
      <Color x:Key="TertiaryContainerColor">#E0E5EC</Color>
      <Color x:Key="OnTertiaryContainerColor">#2D3748</Color>
      <Color x:Key="ErrorColor">#B3261E</Color>
      <Color x:Key="ErrorContainerColor">#F9DEDC</Color>
      <Color x:Key="OnErrorColor">#FFFFFF</Color>
      <Color x:Key="OnErrorContainerColor">#410E0B</Color>
      <Color x:Key="BackgroundColor">#E0E5EC</Color>
      <Color x:Key="OnBackgroundColor">#2D3748</Color>
      <Color x:Key="SurfaceColor">#E0E5EC</Color>
      <Color x:Key="OnSurfaceColor">#2D3748</Color>
      <Color x:Key="SurfaceVariantColor">#E0E5EC</Color>
      <Color x:Key="OnSurfaceVariantColor">#5A6578</Color>
      <Color x:Key="OutlineColor">#A3B1C6</Color>
      <Color x:Key="OnSurfaceInverseColor">#E0E5EC</Color>
      <Color x:Key="SurfaceInverseColor">#2D3748</Color>
      <Color x:Key="PrimaryInverseColor">#A3B1C6</Color>
      <Color x:Key="SurfaceTintColor">#4A5568</Color>
      <Color x:Key="OutlineVariantColor">#A3B1C6</Color>
    </ResourceDictionary>
    <ResourceDictionary x:Key="Dark">
      <!-- Keep dark theme similar for now -->
      <Color x:Key="NeumorphicBgColor">#E0E5EC</Color>
      <Color x:Key="NeumorphicShadowDarkColor">#A3B1C6</Color>
      <Color x:Key="NeumorphicShadowLightColor">#FFFFFF</Color>
      <Color x:Key="NeumorphicTextPrimaryColor">#2D3748</Color>
      <Color x:Key="NeumorphicTextSecondaryColor">#5A6578</Color>
      <Color x:Key="NeumorphicTextMutedColor">#8A95A5</Color>
      <Color x:Key="NeumorphicAccentColor">#4A5568</Color>

      <Color x:Key="PrimaryColor">#4A5568</Color>
      <Color x:Key="OnPrimaryColor">#FFFFFF</Color>
      <Color x:Key="PrimaryContainerColor">#E0E5EC</Color>
      <Color x:Key="OnPrimaryContainerColor">#2D3748</Color>
      <Color x:Key="SecondaryColor">#5A6578</Color>
      <Color x:Key="OnSecondaryColor">#FFFFFF</Color>
      <Color x:Key="SecondaryContainerColor">#E0E5EC</Color>
      <Color x:Key="OnSecondaryContainerColor">#2D3748</Color>
      <Color x:Key="TertiaryColor">#8A95A5</Color>
      <Color x:Key="OnTertiaryColor">#FFFFFF</Color>
      <Color x:Key="TertiaryContainerColor">#E0E5EC</Color>
      <Color x:Key="OnTertiaryContainerColor">#2D3748</Color>
      <Color x:Key="ErrorColor">#FFB4AB</Color>
      <Color x:Key="ErrorContainerColor">#93000A</Color>
      <Color x:Key="OnErrorColor">#690005</Color>
      <Color x:Key="OnErrorContainerColor">#FFDAD6</Color>
      <Color x:Key="BackgroundColor">#E0E5EC</Color>
      <Color x:Key="OnBackgroundColor">#2D3748</Color>
      <Color x:Key="SurfaceColor">#E0E5EC</Color>
      <Color x:Key="OnSurfaceColor">#2D3748</Color>
      <Color x:Key="SurfaceVariantColor">#E0E5EC</Color>
      <Color x:Key="OnSurfaceVariantColor">#5A6578</Color>
      <Color x:Key="OutlineColor">#A3B1C6</Color>
      <Color x:Key="OnSurfaceInverseColor">#E0E5EC</Color>
      <Color x:Key="SurfaceInverseColor">#2D3748</Color>
      <Color x:Key="PrimaryInverseColor">#A3B1C6</Color>
      <Color x:Key="SurfaceTintColor">#4A5568</Color>
      <Color x:Key="OutlineVariantColor">#A3B1C6</Color>
    </ResourceDictionary>
  </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
